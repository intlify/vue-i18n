{
  "name": "@intlify/core-base",
  "description": "@intlify/core-base",
  "version": "12.0.0-alpha.3",
  "author": {
    "name": "kazuya kawaguchi",
    "email": "kawakazu80@gmail.com"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/kazupon",
  "bugs": {
    "url": "https://github.com/intlify/vue-i18n/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intlify/vue-i18n.git",
    "directory": "packages/core"
  },
  "keywords": [
    "core",
    "fundamental",
    "i18n",
    "internationalization",
    "intlify"
  ],
  "homepage": "https://github.com/intlify/vue-i18n/tree/master/packages/core-base#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 16"
  },
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "module": "dist/core-base.js",
  "unpkg": "dist/core-base.global.js",
  "jsdelivr": "dist/core-base.global.js",
  "exports": {
    ".": {
      "types": "./dist/core-base.d.ts",
      "import": "./dist/core-base.js",
      "browser": "./dist/core-base.esm-browser.js",
      "node": {
        "import": {
          "production": "./dist/core-base.prod.js",
          "development": "./dist/core-base.js",
          "default": "./dist/core-base.js"
        }
      }
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "types": "dist/core-base.d.ts",
  "dependencies": {
    "@intlify/message-compiler": "workspace:*",
    "@intlify/shared": "workspace:*"
  },
  "devDependencies": {
    "@intlify/devtools-types": "workspace:*"
  },
  "buildOptions": {
    "name": "IntlifyCoreBase",
    "formats": [
      "mjs",
      "browser",
      "global"
    ]
  }
}
