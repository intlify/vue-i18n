{
  "name": "petite-vue-i18n",
  "description": "Vue I18n lite version",
  "version": "12.0.0-alpha.3",
  "author": {
    "name": "kazuya kawaguchi",
    "email": "kawakazu80@gmail.com"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/kazupon",
  "bugs": {
    "url": "https://github.com/intlify/vue-i18n/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intlify/vue-i18n.git",
    "directory": "packages/petite-vue-i18n"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "intlify",
    "lite",
    "plugin",
    "vue",
    "vue.js"
  ],
  "homepage": "https://github.com/intlify/vue-i18n/tree/master/packages/petite-vue-i18n#readme",
  "engines": {
    "node": ">= 16"
  },
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "module": "dist/petite-vue-i18n.js",
  "unpkg": "dist/petite-vue-i18n.global.js",
  "jsdelivr": "dist/petite-vue-i18n.global.js",
  "exports": {
    ".": {
      "types": "./dist/petite-vue-i18n.d.ts",
      "import": "./dist/petite-vue-i18n.js",
      "browser": "./dist/petite-vue-i18n.esm-browser.js",
      "node": {
        "import": {
          "production": "./dist/petite-vue-i18n.node.js",
          "development": "./dist/petite-vue-i18n.node.js",
          "default": "./dist/petite-vue-i18n.node.js"
        }
      }
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "types": "dist/petite-vue-i18n.d.ts",
  "dependencies": {
    "@intlify/core-base": "workspace:*",
    "@intlify/shared": "workspace:*",
    "@intlify/vue-i18n-core": "workspace:*",
    "@vue/devtools-api": "^6.5.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@intlify/devtools-types": "workspace:*"
  },
  "buildOptions": {
    "name": "PetiteVueI18n",
    "formats": [
      "mjs",
      "mjs-node",
      "mjs-runtime",
      "mjs-node-runtime",
      "browser",
      "browser-runtime",
      "global",
      "global-runtime"
    ]
  }
}
