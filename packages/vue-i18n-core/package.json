{
  "name": "@intlify/vue-i18n-core",
  "description": "@intlify/vue-i18n-core",
  "version": "12.0.0-alpha.3",
  "author": {
    "name": "kazuya kawaguchi",
    "email": "kawakazu80@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/intlify/vue-i18n/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intlify/vue-i18n.git",
    "directory": "packages/vue-i18n-core"
  },
  "keywords": [
    "core",
    "fundamental",
    "i18n",
    "internationalization",
    "intlify",
    "vue",
    "vue.js"
  ],
  "homepage": "https://github.com/intlify/vue-i18n/tree/master/packages/vue-i18n-core#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 16"
  },
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "module": "dist/vue-i18n-core.mjs",
  "unpkg": "dist/vue-i18n-core.global.js",
  "jsdelivr": "dist/vue-i18n-core.global.js",
  "exports": {
    ".": {
      "types": "./dist/vue-i18n-core.d.ts",
      "import": "./dist/vue-i18n-core.js",
      "browser": "./dist/vue-i18n-core.esm-browser.js",
      "node": {
        "import": {
          "development": "./dist/vue-i18n-core.js",
          "default": "./dist/vue-i18n-core.js"
        }
      }
    },
    "./petite": {
      "types": "./dist/petite-vue-i18n-core.d.ts",
      "import": "./dist/petite-vue-i18n-core.js",
      "browser": "./dist/petite-vue-i18n-core.esm-browser.js",
      "node": {
        "import": {
          "development": "./dist/petite-vue-i18n-core.js",
          "default": "./dist/petite-vue-i18n-core.js"
        }
      }
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "types": "dist/vue-i18n-core.d.ts",
  "dependencies": {
    "@intlify/core-base": "workspace:*",
    "@intlify/shared": "workspace:*",
    "@vue/devtools-api": "^8.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@intlify/devtools-types": "workspace:*"
  },
  "buildOptions": {
    "name": "IntlifyVueI18nCore",
    "formats": [
      "mjs",
      "browser",
      "global"
    ]
