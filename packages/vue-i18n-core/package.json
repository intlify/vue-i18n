{
  "name": "@intlify/vue-i18n-core",
  "version": "9.2.0-beta.22",
  "description": "@intlify/vue-i18n-core",
  "keywords": [
    "core",
    "fundamental",
    "i18n",
    "internationalization",
    "intlify",
    "vue",
    "vue.js"
  ],
  "license": "MIT",
  "author": {
    "name": "kazuya kawaguchi",
    "email": "kawakazu80@gmail.com"
  },
  "homepage": "https://github.com/intlify/vue-i18n-next/tree/master/packages/vue-i18n-core#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intlify/vue-i18n-next.git",
    "directory": "packages/vue-i18n-core"
  },
  "bugs": {
    "url": "https://github.com/intlify/vue-i18n-next/issues"
  },
  "files": [
    "index.js",
    "index.mjs",
    "dist"
  ],
  "main": "index.js",
  "module": "dist/vue-i18n-core.esm-bundler.js",
  "unpkg": "dist/vue-i18n-core.global.js",
  "jsdelivr": "dist/vue-i18n-core.global.js",
  "types": "dist/vue-i18n-core.d.ts",
  "dependencies": {
    "@intlify/core-base": "9.2.0-beta.22",
    "@intlify/shared": "9.2.0-beta.22",
    "@intlify/vue-devtools": "9.2.0-beta.22",
    "@vue/devtools-api": "^6.0.0-beta.13"
  },
  "devDependencies": {
    "@intlify/devtools-if": "9.2.0-beta.22"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "engines": {
    "node": ">= 12"
  },
  "buildOptions": {
    "name": "IntlifyVueI18nCore",
    "formats": [
      "esm-bundler",
      "esm-browser",
      "cjs",
      "global"
    ]
  },
  "exports": {
    ".": {
      "import": {
        "node": "./index.mjs",
        "default": "./dist/vue-i18n-core.esm-bundler.js"
      },
      "require": "./index.js"
    },
    "./dist/*": "./dist/*",
    "./index.mjs": "./index.mjs",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
